@model Reservation
@{ Layout = "_Layout"; ViewBag.Title = "All Reservations";}

<h2>All Reservations</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.success</td>
            <td>@Model.timestamp</td>
            <td>@Model.date</td>
            <td>@Model.@base"</td>
            <td>@Model.rates</td>
        </tr>
        
    </tbody>
</table>