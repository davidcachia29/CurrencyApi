@model CurrencyData
@{ Layout = "_Layout"; ViewBag.Title = "All Reservations";}

<h2>Currency Converter API</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>Successful</th>
            <th>TimeStamp</th>
            <th>Date</th>
            <th>Base</th>
            <th>Rates</th>
            
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.success</td>
            <td>@Model.timestamp</td>
            <td>@Model.date</td>
            <td>@(Model.@base)</td>

            @if (Model.rates == null)
            {
                <td>Error</td>
            }
            else
            {
                foreach (var r in Model.rates)
                {
                    <td>@r</td>
                }
            }



        </tr>
        
    </tbody>
</table>